# Токен Telegram бота
# Получите токен у @BotFather в Telegram
BOT_TOKEN=your_telegram_bot_token_here

# Настройки для YandexGPT (опционально)
# Если используете YandexGPT вместо локальной модели
YANDEX_API_KEY=your_yandex_api_key_here
YANDEX_FOLDER_ID=your_yandex_folder_id_here

# Настройки базы данных (опционально)
# По умолчанию используется SQLite с файлом finance.db
DB_PATH=finance.db

# Настройки логирования (опционально)
LOG_LEVEL=INFO

# Настройки для парсинга данных (опционально)
# Интервал обновления данных о вкладах в секундах (по умолчанию 24 часа)
DEPOSITS_UPDATE_INTERVAL=86400

# Настройки для инвестиций (опционально)
# Количество топ акций для отображения
TOP_STOCKS_COUNT=5

# Настройки для кредитного калькулятора (опционально)
# Максимальный срок кредита в месяцах
MAX_CREDIT_TERM=120 